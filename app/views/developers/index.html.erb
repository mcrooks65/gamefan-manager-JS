<h1>Developer Index</h1>
<table class="table">
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <tr>
      <td><strong>Name</strong></td>
      <td><strong>Employees</strong></td>
      <td><strong>Location</strong></td>
    </tr>
    <% @developers.each do |developer| %>
      <tr>
    
        <td><%= link_to "#{developer.name}", developer_path(developer) %></td>
        <td><%= developer.employees %></td>
        <td><%= developer.location %></td>
        <% end %>
      </tr>
  </tbody>
</table>
<br> <br>
<%= link_to "Back to Home", '/' %>
